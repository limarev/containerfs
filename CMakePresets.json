{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "include": [
    "BasePresets.json"
  ],
  "configurePresets": [
    {
      "name": "build-release",
      "inherits": [
        "default",
        "vcpkg",
        "ninja",
        "debug"
      ]
    }
  ],
  "buildPresets": [
    {
      "name": "build-release",
      "configurePreset": "build-release"
    }
  ],
  "testPresets": [
    {
      "name": "build-release",
      "configurePreset": "build-release",
      "inherits": ["default"]
    }
  ],
  "packagePresets": [
    {
      "name": "build-release",
      "configurePreset": "build-release",
      "inherits": [
        "zip"
      ]
    }
  ],
  "workflowPresets": [
    {
      "name": "release",
      "steps": [
        {
          "type": "configure",
          "name": "build-release"
        },
        {
          "type": "build",
          "name": "build-release"
        },
        {
          "type": "test",
          "name": "build-release"
        },
        {
          "type": "package",
          "name": "build-release"
        }
      ]
    }
  ]
}